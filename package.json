{
  "name": "passfortress",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev .",
    "deploy": "wrangler pages deploy .",
    "db:create": "wrangler d1 create passfortress-db",
    "db:schema": "wrangler d1 execute passfortress-db --file=./schema.sql",
    "build:css": "npx tailwindcss -c tailwind.config.js -i ./public/tailwind.input.css -o ./public/tailwind.generated.css --minify"
  },
  "dependencies": {
    "itty-router": "^4.0.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240423.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.0.0",
    "wrangler": "^4.51.0"
  }
}
